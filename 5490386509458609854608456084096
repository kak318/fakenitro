	<script>
    function makeid(length) {
    var result           = '';
    var characters       = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
    var charactersLength = characters.length;
    for ( var i = 0; i < length; i++ ) {
      result += characters.charAt(Math.floor(Math.random() * 
 charactersLength));
   }
   return result;
}

console.log(makeid(5));
		var text = document.getElementById('text');
var greeting = ['Creating space on server...',
                'Starting free nitro generator...',
                'Starting spaceNR9048509438509385...',
                'Attempting free nitro generation...',
                'err: in: line: 23489320 file: NG...',
                'err: in: line: 54654665 file: NG...',
                'err: in: line: 63565477 file: NG...',
                'LOADING NGEN...',
                'INFO: NGEN STARTED',
                'INFO: NGEN 1%',
                'INFO: NGEN 15%',
                'INFO: NGEN 40%',
                'INFO: NGEN 99%',
                'INFO: FINALIZING...',
                'INFO: NGEN EIXT',
                'Generated free nitro code: http://discord.gift/' + makeid(30)]
text.innerHTML = '<i>â–®</i>';

(function greet() {
    //if (greeting.length > 0 && greeting.length < 4) {
    //}
    var line = greeting.shift();
    if (!line) {
        return;
    }
    line = line.split('');
    (function type() {
        var character = line.shift();
        if (!character) {
            text.insertBefore(document.createElement('br'), text.lastChild);
            return setTimeout(greet, 2500);
        }

        text.insertBefore(document.createTextNode(character), text.lastChild);
        setTimeout(type, 1);
    }());
}());

	</script>
